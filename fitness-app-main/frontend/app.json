{
  "expo": {
    "name": "Fit Desert",
    "slug": "fit-desert",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/android-chrome-192x192.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "icon": "./assets/images/android-chrome-512x512.png",
    "scheme": "fitdesert",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/android-chrome-512x512.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-chrome-512x512.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yourname.fitdesert",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-32x32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/android-chrome-192x192.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "87129fd0-160c-4a68-b4cd-bd8ac24a7077"
      }
    }
  }
}
